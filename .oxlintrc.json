{
  "plugins": [
    "oxc",
    "typescript",
    "unicorn",
    "import",
    "promise",
    "vitest"
  ],
  "categories": {
    "correctness": "error",
    "suspicious": "warn",
    "perf":"warn"
  },
  "rules": {
    "unicorn/require-post-message-target-origin": "off",
    "unicorn/prefer-add-event-listener": "off"
  }
}